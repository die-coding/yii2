{
	"name": "diecoding/yii2-core",
	"description": "Yii2 Core Development Package - Tidak ada dokumentasi, karena hanya untuk tambahan internal Die Coding saja ^_^",
	"type": "yii2-extension",
	"keywords": [
		"yii2",
		"framework",
		"extension",
		"yii2-core"
	],
	"license": "BSD-3-Clause",
	"support": {
		"issues": "https://github.com/die-coding/yii2-core/issues",
		"source": "https://github.com/die-coding/yii2-core"
	},
	"require": {
		"php": ">=7.0",
        "yiisoft/yii2": "~2.0.14",
		"yiisoft/yii2-bootstrap": "~2.0.0",
		"yiisoft/yii2-bootstrap4": "@dev",
		
        "diecoding/yii2-rbac": "@dev",
        "diecoding/yii2-toastr": "@dev"
	},
	"require-dev": {
		"phpunit/phpunit": "<7"
	},
	"autoload": {
		"psr-4": {
			"diecoding\\": "src"
		}
	},
	"authors": [
		{
			"name": "Die Coding",
			"email": "diecoding@gmail.com"
		},
		{
			"name": "Sugeng Sulistiyawan",
			"email": "sugeng.sulistiyawan@gmail.com"
		}
	],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
